{
  "name": "matchmyroute-api",
  "description": "MatchMyRoute Backend API",
  "version": "0.1.0",
  "private": true,
  "license": "Commercial",
  "author": "GradTeam",
  "engines": {
    "node": "^6.0.0"
  },
  "config": {
    "host": "http://localhost",
    "port": "8080",
    "with_services": "true"
  },
  "scripts": {
    "build": "gulp build",
    "clean": "npm prune && gulp clean",
    "swagger-ui": "gulp swagger-ui",
    "debug": "gulp debug --script build/app.js --with_services $npm_package_config_with_services",
    "deploy": "gulp build && gcloud app deploy -q build/app.yaml",
    "gulp": "gulp",
    "lint": "gulp tslint",
    "serve": "gulp serve --script build/app.js --with_services $npm_package_config_with_services",
    "start": "node app.js",
    "generator": "plop",
    "test": "gulp test",
    "dredd": "dredd $npm_package_config_host:$npm_package_config_port/swagger.yaml $npm_package_config_host:$npm_package_config_port",
    "gulp:dredd": "gulp dredd --script build/app.js",
    "unittest": "gulp unittest",
    "e2etest": "gulp e2etest",
    "preinstall": "npm update",
    "postinstall": "npm prune"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "cache-manager": "^2.2.0",
    "cache-manager-memcached-store": "^2.1.0",
    "data.maybe": "^1.2.2",
    "es6-promisify": "^5.0.0",
    "joi": "^10.0.0",
    "jsonwebtoken": "^7.3.0",
    "koa": "^1.2.4",
    "koa-cors": "0.0.16",
    "koa-qs": "^2.0.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.17.2",
    "node-yaml": "^3.0.3",
    "pg": "^6.1.3",
    "qs": "^6.4.0",
    "ramda": "^0.23.0",
    "request": "^2.79.0",
    "seneca": "^3.2.2",
    "seneca-web": "^2.0.0",
    "seneca-web-adapter-koa1": "^1.0.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "@types/joi": "^10.0.0",
    "@types/koa": "^2.0.37",
    "@types/lodash": "^4.14.50",
    "@types/node": "^7.0.0",
    "@types/request": "0.0.39",
    "@types/seneca": "^2.1.5",
    "@types/winston": "^2.2.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "dredd": "^2.2.5",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-cli": "^1.2.2",
    "gulp-env": "^0.4.0",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^4.2.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-replace": "^0.5.4",
    "gulp-run": "^1.7.1",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-tslint": "^6.1.3",
    "gulp-typescript": "^3.1.3",
    "istanbul": "^0.4.5",
    "jsonfile": "^2.4.0",
    "mocha": "^3.2.0",
    "remap-istanbul": "^0.8.2",
    "run-sequence": "^1.2.2",
    "should": "^11.2.1",
    "swagger-ui": "^2.2.10",
    "tslint": "^3.15.1",
    "typescript": "^2.1.5"
  }
}
